<table id="mapTable">
	<tbody>
		{{#each layers.openedTips}}
		<tr data-y={{@index}}>
			{{#each this}}
				<td data-x={{@index}}>
					{{#if opened}}
						<div class="{{className}}">
								<p>{{tip}}</p>
						</div>	
					{{/if}}
					{{^if opened}}
						{{#if flaged}}
							<div class="flag">
								<p>C</p>
							</div>	
						{{/if}}
						{{^if flaged}}
							<div></div>
						{{/if}}
					{{/if}}

				</td>
			{{/each}}
		</tr>
		{{/each}}
		<tr getcellvalues="null" style="display: none"></div>
	</tbody>
</table>
<div bindcheck="null"></div>